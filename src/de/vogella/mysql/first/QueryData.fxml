<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: linear-gradient(to bottom, #ff7f50, #6a5acd);" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.vogella.mysql.first.Controller">
   <children>
      <TextArea fx:id="queryList" layoutX="299.0" layoutY="14.0" prefHeight="376.0" prefWidth="287.0" promptText="Selected: " />
      <Button fx:id="showData" layoutX="149.0" layoutY="284.0" mnemonicParsing="false" onAction="#queryData" text="View Data" />
      <Button fx:id="queryBack" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#queryBack_click" text="Back" />
      <ComboBox fx:id="view" layoutX="108.0" layoutY="39.0" prefWidth="150.0" promptText="Select Values" />
      <Button fx:id="add" layoutX="140.0" layoutY="85.0" mnemonicParsing="false" onAction="#add_click" prefHeight="25.0" prefWidth="87.0" text="Add">
         <effect>
            <DropShadow color="#c65757" />
         </effect></Button>
      <Button fx:id="tableView" layoutX="140.0" layoutY="324.0" mnemonicParsing="false" onAction="#viewTable_click" text="View in Table" />
   </children>
</AnchorPane>
